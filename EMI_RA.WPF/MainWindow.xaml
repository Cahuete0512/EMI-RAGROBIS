<Window x:Class="EMI_RA.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EMI_RA.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">



    <StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button x:Name="BouttonAdherent" Click="voirAdherent">Adhérent</Button>
            <Button x:Name="BouttonFournisseurs" Click="voirFournisseur">Fournisseurs</Button>
            <Button x:Name="BouttonPanier" Click="voirPanier">Panier</Button>
            <Button x:Name="BouttonPanier1" Click="voirPanier1">Panier</Button>
        </StackPanel>
        <Frame x:Name="Main" Margin="-10,0,10,0" />

    </StackPanel>

</Window>
